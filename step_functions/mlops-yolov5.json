{
  "StartAt": "Start Training Job",
  "States": {
    "Failed": {
      "Type": "Fail"
    },
    "Start Training Job": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:ap-northeast-2:687314952804:function:lambda-yolo5-training",
      "ResultPath": "$",
      "Next": "Check Status Training"
    },
    "Check Status Training": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:ap-northeast-2:687314952804:function:lambda-await",
      "ResultPath": "$",
      "Next": "Check Training Branch"
    },
    "Check Training Branch": {
      "Type": "Choice",
      "Choices": [
        {
          "Or": [
            {
              "Variable": "$.status",
              "StringEquals": "Completed"
            }
          ],
          "Next": "Check Accuracy"
        },
        {
          "Or": [
            {
              "Variable": "$.status",
              "StringEquals": "InProgress"
            }
          ],
          "Next": "Wait Status Training"
        }
      ]
    },
    "Wait Status Training": {
      "Type": "Wait",
      "Seconds": 30,
      "Next": "Check Status Training"
    },
    "Check Accuracy": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:ap-northeast-2:687314952804:function:lambda-check-acc",
      "ResultPath": "$",
      "Next": "Accuracy above threshold?"
    },
    "Accuracy above threshold?": {
      "Comment": "A Choice state checks accuracy",
      "Type": "Choice",
      "Choices": [
        {
          "Variable": "$.train_result",
          "StringEquals": "PASS",
          "Next": "Register Model"
        },
        {
          "Variable": "$.train_result",
          "StringEquals": "FAIL",
          "Next": "Failed"
        }
      ],
      "Default": "Failed"
    },
    "Register Model": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:ap-northeast-2:687314952804:function:lambda-model-registry",
      "ResultPath": "$",
      "Next": "End Deployment"
    },
    "End Deployment": {
      "Type": "Succeed"
    }
  }
}